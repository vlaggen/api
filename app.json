{
    "name": "Vlaggen API",
    "description": "An API to retrieve parameters",
    "repository": "https://github.com/vlaggen/api",
    "keywords": [
        "vapor",
        "remote-config",
        "feature-flags",
        "flags"
    ],
    "env": {
        "UNVERIFIED_TLS": {
            "description": "Unverified TLS is required to be true if you are deploying Heroku Postgres's standard plan",
            "value": "true"
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ],
    "buildpacks": [
        {
            "url": "vapor/vapor"
        }
    ]
}
